<Window x:Class="EggFarmSystem.Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" MinHeight="500" MinWidth="800" Background="#0C0C0C"
        WindowState="Maximized" WindowStyle="None" Style="{StaticResource Window.MainWindowKey}"
        >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"></RowDefinition>
			<RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Menu>
            <MenuItem Header="DASHBOARD" />
            <MenuItem Header="PAKAN" />
            <MenuItem Header="PENYUSUTAN" />
            <MenuItem Header="PRODUKSI" />
            <MenuItem Header="ABSENSI" />            
            <MenuItem Header="MASTER" IsChecked="True" />
            <MenuItem Header="SETTINGS" />
            <MenuItem Header="LAPORAN" />
        </Menu>
    </Grid>
</Window>
